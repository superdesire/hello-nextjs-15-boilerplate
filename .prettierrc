{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "singleAttributePerLine": true,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "quoteProps": "consistent",
  "arrowParens": "always",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
